# USB port
port: /dev/ttyUSB0
# Tf frame id
frame_id: imu_link
# Data reading frequency
freqency: 200
# Sensor operation mode --> modes could be found in sensor datasheet
operation_mode: 0x08
# Calibration byte sequence
calibration: [251, 255, 223, 255, 2, 0, 250, 254, 4, 0, 105, 1, 0, 0, 255, 255, 0, 0, 232, 3, 97, 3]

